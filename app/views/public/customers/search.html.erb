<div class="container">
  <div class="row justify-content-end">
    <div class="col-3">
      <%= search_form_for @q, url: search_customers_path do |f| %>
        <%= f.search_field :nickname_cont, placeholder: "ユーザー検索" %>
        <%= f.submit '検索' %>
      <% end %>
    </div>
  </div>
  <div class="row">
    <h3 class="py-3">検索結果</h3>
  </div>
  <%= render 'public/customers/customers_index', obj: @results %>
</div>
